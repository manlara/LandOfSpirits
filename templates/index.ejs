    <div class="section-crew">

        <% for(var i=0; i<crewPics.length; i++) {%>
            <%if (i==0 || (i+1)%4==0 && (i+1)!=crewPics.length){%>
                <div class="row">
            <%}%>
            <div class="col-md-3">
