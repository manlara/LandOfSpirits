                <div class="crew-descr">
                    <span><%=crewName[i]%></span>
                    <p><%=crewDesc[i]%></p>
                </div>
            </div>
            <%if ((i+1)%4==0) {%>
                </div>
            <%}%>
        <% } %>

    </div>
        

